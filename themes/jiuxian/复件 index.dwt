				}
				var endtime_box_obj = endtime_obj[i].getElementsByTagName('div');
				
				for(j=0;j<endtime_box_obj.length;j++)
				{
					
					if(endtime_box_obj[j].className == "group_endtime_box")
					{
							
						endtime_box_obj[j].innerHTML = str;
					}
				}
			}
		}
     
    }, 100);

